{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "stablecog",
  "title": "Stablecog",
  "description": "Create amazing art with AI in seconds.",
  "icon": "stablecog.png",
  "author": "yektagg",
  "owner": "stablecog",
  "categories": [
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "imagine",
      "title": "Imagine",
      "subtitle": "Create art with AI",
      "description": "Create art with AI",
      "mode": "view",
      "arguments": [
        {
          "name": "Prompt",
          "placeholder": "Prompt",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2",
    "@raycast/utils": "^1.9.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
